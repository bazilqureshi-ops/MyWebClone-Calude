@using Emse.QMagic.Web.Models;
@{
    ViewBag.Title = "Index";
    Layout = null;
    List<qmService> Services = ViewBag.Services;
    string times = "";

    foreach (qmService service in Services)
    {
        times += service.ResetTime.Hours + ":" + service.ResetTime.Minutes + ":" + service.ResetTime.Seconds+",";
        times += service.Reset2Time.Hours + ":" + service.Reset2Time.Minutes + ":" + service.Reset2Time.Seconds+",";
    }
}

<div class="tab-pane active" id="tab1" name="tab1" style="background-color:#000; font-family:'Courier New'; font-size:11px; color:#FFF; width:100%; height:100%; position:absolute; left:0px; top:0px; overflow-y:scroll;">

</div>

<script src="~/Scripts/jquery331.js"></script>
<script src="~/Scripts/jquery.signalR-2.4.3.min.js"></script>
<script src="~/SignalR/Hubs"></script>
<script src="~/Scripts/WebKeeper.js"></script>
<link href="~/assets/jquery-ui/jquery-ui.css" rel="stylesheet" />
<script src="~/assets/jquery-ui/jquery-ui.js"></script>

<script>
    var fullstring = "@(times)";
    var ResetTimes = fullstring.split(",");

    const distinct = (value, index, self) => {
        return self.indexOf(value) === index;
    }

    ResetTimes = ResetTimes.filter(distinct);
	
	setInterval(function(){
		var date = new Date();
		var rightnow = date.getHours() + ":" + date.getMinutes() + ":" + date.getSeconds();
		
		ResetTimes.forEach(function(item){
            if (rightnow == item) {

				$.get("Diagnose/ResetTickets", { ResetTime:item }, function (data) {
					data = data.replace(/\s/g, '');

					signalRHub.server.send(data).fail(function (e) {
						$("#tab1").append(e.message + "<br/>");
					});
				});
			}
		});
    }, 1000);
    
</script>