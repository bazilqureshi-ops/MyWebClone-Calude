@using Emse.QMagic.Web.Models;
@{
    ViewBag.Title = "Virtual Terminal";
    Layout = "~/Views/Shared/_Layout.cshtml";


    List<qmTicket> WaitingList = ViewBag.WaitingList;
    List<qmService> ServiceList = ViewBag.ServiceList;
    List<qmSegment> SegmentList = ViewBag.SegmentList;
    qmTerminal LoginTerminal = ViewBag.LoginTerminal;
    List<qmTerminal> TransferTerminal = ViewBag.TerminalTransfer;
    List<qmSegment> TransferService = ViewBag.ServiceTransfer;
    List<qmStatus> Statuses = ViewBag.Statuses;
    string Segments = ViewBag.Segments;
    List<qmTerminalConnection> tco = ViewBag.tco;
    List<qmUser> userlist = ViewBag.userlist;
    List<qmBranch> branches = ViewBag.branches;
    List<qmTerminal> allterminals = ViewBag.allterminals;
    bool Maintenance = ViewBag.Maintenance;
    List<qmAutoTransferRule> ATRs = ViewBag.ATRs;

    string LanguageCode = Session["MagicLanguage"] != null ? Session["MagicLanguage"].ToString() : "";
    if (LanguageCode == null)
    {
        LanguageCode = "";
    }
}
@{
    qmTicket autofillticket = null;
    if (ViewBag.AutoFillTicket != null)
    {
        autofillticket = ViewBag.AutoFillTicket;
    }
}


@section scripts{
    <script src="~/Scripts/jquery.signalR-2.4.3.min.js"></script>
    <script src="~/SignalR/Hubs"></script>
    <script>
        var loginuserID = "@(HttpContext.Current.Session["MagicUser"].ToString())";
        var terminalID = "@(LoginTerminal.TerminalID)";
        var commasegment = "@(Segments)";
        var SegmentList = commasegment.split(",");
        var mymacroid = "@(LoginTerminal.MacroID)";

        $(document).ready(function () {
            MyVotebookID = "@(ViewBag.RelatedVotebook)";
            IsVotebookEnabled = @(ViewBag.IsVotebookEnabled.ToString().ToLower());
        });
    </script>
    <style>
        .MnItem {
            height: 23px;
            padding: 5px;
            padding-bottom: 10px;
            padding-top: 10px;
            width: 100%;
            border-bottom: dotted 1px #CCC;
            cursor: pointer;
            font-size: 13px;
        }

            .MnItem:hover {
                background-color: #F5F5F5;
            }


        .blurbg {
            width: 100%;
            height: 100%;
            position: fixed;
            margin: 0px;
            padding: 0px;
            left: 0px;
            top: 0px;
            background-image: url(../assets/images/blur.png);
            display: none;
            z-index: 999998;
        }

        .normal-shadow {
            -webkit-box-shadow: 3px 3px 5px 6px #ccc; /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
            -moz-box-shadow: 3px 3px 5px 6px #ccc; /* Firefox 3.5 - 3.6 */
            box-shadow: 3px 3px 5px 6px #ccc; /* Opera 10.5, IE 9, Firefox 4+, Chrome 6+, iOS 5 */
        }

        .popupwindow {
            display: none;
            margin-top: 20px;
            background-color: #FFFFFF;
            border-radius: 5px;
            position: absolute;
            z-index: 1450;
            border: solid 2px #CCC;
        }

        .ekselect {
            border: solid 1px #CCCC;
            padding: 12px;
        }
    </style>
    <script src="~/Scripts/WebTerminal.js"></script>
    <link href="~/assets/css/designer.css" rel="stylesheet" />

    <link href="~/assets/css/hand-terminal.css" rel="stylesheet" />
    <link href="~/assets/jquery-ui/jquery-ui.css" rel="stylesheet" />
    <script src="~/assets/jquery-ui/jquery-ui.js"></script>

    <link href="~/assets/css/sweetalert2.min.css" rel="stylesheet" />
    <script src="~/assets/js/sweetalert2.all.min.js"></script>

    @{
        if (autofillticket != null)
        {
            <script>
                var bckp = "@(autofillticket.TicketID)";
                if (bckp != null) {
                    BackupCall = bckp;
                }

                $(document).ready(function () {
                    if (BackupCall != null) {
                        //ManualCall(BackupCall);
                    }
                });
            </script>
        }

<script>
            $(document).ready(function () {
                SetEndStatuses('@(Statuses.Where(x=>x.StatusID == LoginTerminal.CloseStatusID).FirstOrDefault().StatusName)','@(Statuses.Where(x=>x.StatusID == LoginTerminal.CloseStatusID).FirstOrDefault().StatusColor)');
                SetNextCallStatuses('@(Statuses.Where(x=>x.StatusID == LoginTerminal.NextCallStatusID).FirstOrDefault().StatusName)', '@(Statuses.Where(x=>x.StatusID == LoginTerminal.NextCallStatusID).FirstOrDefault().StatusColor)');

                SetReadyStatus('@(Statuses.Where(x=>x.StatusID == LoginTerminal.ReadyStatusID).FirstOrDefault().StatusName)', '@(Statuses.Where(x=>x.StatusID == LoginTerminal.ReadyStatusID).FirstOrDefault().StatusColor)', @(Statuses.Where(x=>x.StatusID == LoginTerminal.ReadyStatusID).FirstOrDefault().StatusID));

                SetExitStatus('@(Statuses.Where(x=>x.StatusID == LoginTerminal.ExitStatusID).FirstOrDefault().StatusName)', '@(Statuses.Where(x=>x.StatusID == LoginTerminal.ExitStatusID).FirstOrDefault().StatusColor)', @(Statuses.Where(x=>x.StatusID == LoginTerminal.ExitStatusID).FirstOrDefault().StatusID));
            });
</script>
    }


}



