<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="FictiveTicketModal" id="FictiveTicketModal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="gridSystemModalLabel">
                    Services
                </h4>
            </div>
            <div class="modal-body">
                <div class="row">

                    <div class="col-md-12">
                        <div class="col-md-6">
                            Branch
                            <select name="genBranch" id="genBranch" class="designer-select ekselect form-control">
                                @{
                                    foreach (qmBranch item in branches)
                                    {

                                        if (item.BranchID == LoginTerminal.BranchID)
                                        {
                                            <option value="@(item.BranchID)" selected="selected">@(item.BranchName) (This)</option>
                                        }
                                        else
                                        {
                                            <option value="@(item.BranchID)">@(item.BranchName)</option>
                                        }
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-md-6">
                            Automatic Ticket Routing
                            <select name="genATR" id="genATR" class="designer-select ekselect form-control">
                                <option value="">No Routing</option>
                                @{
                                    foreach (qmAutoTransferRule item in ATRs)
                                    {
                                        <option value="@(item.AutoTransferRuleID)">@(item.AutoTransferName)</option>
                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <div class="col-md-12" style="margin-top:20px;">
                        <div class="col-xs-6 col-md-6">
                            Service
                            <select name="genService" id="genService" class="designer-select ekselect form-control">
                                @{
                                    foreach (qmService item in ServiceList)
                                    {
                                        <option value="@(item.ServiceID)">@(item.ServiceName)</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-xs-6 col-md-6">
                            Segment
                            <select name="genSegment" id="genSegment" class="designer-select ekselect form-control">
                                <option value="0" ServiceID="99999999" class="SegmentItem">Select Service First</option>
                                @{
                                    foreach (qmSegment item in SegmentList)
                                    {
                                        <option value="@(item.SegmentID)" ServiceID="@(item.ServiceID)" class="SegmentItem">@(item.SegmentName)</option>
                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <div class="col-md-12 text-center" style="margin-top:20px;">
                        <button class="btn btn-default" onclick="ToggleGeneratorCustomerSelector();" id="FictiveCustomerInfo" custID="">No Customer Selected</button>
                    </div>
                    <div class="col-md-12" style="margin-top:20px; display:none;" id="genCustomerSelectorDiv">
                        <div class="col-md-8">
                            <input type="text" class="form-control" id="genCustomerSearch" name="genCustomerSearch" placeholder="Name, surname, phone number or e-mail address" />
                        </div>
                        <div class="col-md-2">
                            <button class="btn btn-primary" onclick="SearchFictiveCustomer();"><i class="fa fa-search"></i></button>
                        </div>
                        <div class="col-md-2">
                            <a class="btn btn-primary" href="~/Customer/CreateCustomer" target="_blank"><i class="fa fa-plus"></i></a>
                        </div>
                        <div class="col-md-12" id="genCustomerFind">
                            <table width="100%" class="table card-table">
                                <tr>
                                    <td>Unique ID</td>
                                    <td>Name Surname</td>
                                    <td>E-Mail</td>
                                    <td>Phone</td>
                                    <td>Device</td>
                                    <td>Action</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <div id="GenerateResult" class="text-center" style="font-size: 20px; margin-left:200px; margin-top: -280px; border: dotted 1px #CCC; width: 448px; min-height:450px; background-color: #FFF; position: absolute; display: flex; align-items: center; justify-content: center;">

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" style="margin-right:30px;">
                    @{
                        switch (LanguageCode)
                        {
                            case "AR":
                                @Html.Label("اغلاق")
                                break;
                            case "TR":
                                @Html.Label("Kapat")
                                break;
                            case "FR":
                                @Html.Label("Proche")
                                break;
                            case "PK":
                                @Html.Label("بند")
                                break;
                            case "IN":
                                @Html.Label("बंद करना")
                                break;
                            case "PH":
                                @Html.Label("Isara")
                                break;
                            default:
                                @Html.Label("Close")
                                break;
                        }
                    }
                </button>


                <button class="btn btn-primary" id="btnStart1" onclick="GenerateTicket();"><i class="fa fa-tags"></i> Generate</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->


<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="ServiceTransfer" id="TerminalTransfer">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="gridSystemModalLabel">Terminal Transfer</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="TransferList">
                        <table width="100%" border="0">
                            @{
                                if (LoginTerminal.TransferStatusID != null)
                                {
                                    if (TransferTerminal.Count > 0)
                                    {
                                        foreach (qmTerminal item in TransferTerminal)
                                        {
                                            <tr>
                                                <td onclick="TransferToTerminal(@(item.TerminalID), @(LoginTerminal.TerminalID), '@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusName)','@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusColor)');" align="left" class="MnItem">
                                                    @item.TerminalName
                                                </td>
                                            </tr>
                                        }
                                    }
                                }
                            }
                        </table>
                    </div>
                </div>
                <div id="Loader1" class="text-center">
                    <br /><br /><img src="~/assets/images/loading.gif" /><br />
                    <p>Please wait while ticket transferring...</p><br /><br />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="TerminalTransferAutoSelect(@(LoginTerminal.TerminalID), '@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusName)','@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusColor)');">Automatically Select</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->


<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="ServiceTransfer" id="ServiceTransfer">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="gridSystemModalLabel">Service Transfer</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="ServiceList">
                        <table width="100%" border="0">
                            @{
                                int i = 0;
                                foreach (qmSegment item in TransferService)
                                {
                                    <tr>
                                        <td onclick="TransferToService(@(item.SegmentID), @(LoginTerminal.TerminalID), '@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusName)','@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusColor)');" align="left" class="MnItem">
                                            @(ServiceList.Where(x=>x.ServiceID == item.ServiceID).FirstOrDefault().ServiceName) / @(item.SegmentName)
                                        </td>
                                    </tr>

                                    i++;
                                }
                            }
                        </table>
                    </div>
                </div>
                <div id="Loader2" class="text-center">
                    <br /><br /><img src="~/assets/images/loading.gif" /><br />
                    <p>Please wait while ticket transferring...</p><br /><br />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="ServiceTransferAutoSelect(@(LoginTerminal.TerminalID), '@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusName)','@(Statuses.Where(x=>x.StatusID == LoginTerminal.TransferStatusID).FirstOrDefault().StatusColor)');">Automatically Select</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->



